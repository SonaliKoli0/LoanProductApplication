package product;

import java.util.Date;

public class Schedule {
	private int id;
	private Date date;
	private Double amount;
	public int getId() {
		return id;
	}
	public void setId(int id) {
		this.id = id;
	}
	public Date getDate() {
		return date;
	}
	public void setDate(Date date) {
		this.date = date;
	}
	public Double getAmount() {
		return amount;
	}
	public void setAmount(Double amount) {
		this.amount = amount;
	}
	
	public String toString() {
        return 
                "Id=" + getId() +
                ", Date="+getDate()+
                ", Amount=" + getAmount()
                ;
    }
	

}
